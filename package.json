    "react": "~18.3.1",
    "react-dom": "~18.3.1",
    "valtio": "^2.1.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@modern-js/app-tools": "^2.63.0",
    "@modern-js/tsconfig": "^2.63.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.19.67",
    "@types/react": "~18.3.12",
    "@types/react-dom": "~18.3.1",
    "less": "^4.2.1",
    "lint-staged": "^15.2.10",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.2"
  }
